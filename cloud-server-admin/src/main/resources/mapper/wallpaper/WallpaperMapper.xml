<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vip.gpg123.wallpaper.mapper.WallpaperMapper">

    <resultMap id="BaseResultMap" type="vip.gpg123.wallpaper.domain.Wallpaper">
            <id property="id" column="id" />
            <result property="name" column="name" />
            <result property="tags" column="tags" />
            <result property="dirPath" column="dir_path"/>
            <result property="size" column="size" />
            <result property="url" column="url"/>
            <result property="createTime" column="create_time" />
            <result property="createBy" column="create_by" />
            <result property="updateTime" column="update_time" />
            <result property="updateBy" column="update_by" />
    </resultMap>

    <sql id="Base_Column_List">
        id,name,tags,size,create_time,create_by,
        update_time,update_by
    </sql>
</mapper>
